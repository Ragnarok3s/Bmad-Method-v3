{
  "name": "@bmad/api-client",
  "version": "0.1.0",
  "private": false,
  "description": "Shared API client for Bmad Method services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
