    "expo-dev-client": "4.0.16",
    "expo-notifications": "0.28.5",
    "expo-sqlite": "13.2.2",
    "expo-status-bar": "3.0.8",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-safe-area-context": "4.10.5",
