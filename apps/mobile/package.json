    "@react-navigation/native-stack": "7.6.1",
    "expo": "54.0.21",
    "expo-constants": "18.0.10",
    "expo-dev-client": "6.0.16",
    "expo-notifications": "0.32.12",
    "expo-sqlite": "16.0.8",
    "expo-status-bar": "3.0.8",
    "react": "19.2.0",
    "react-native": "0.82.1",
    "react-native-safe-area-context": "5.6.2",
