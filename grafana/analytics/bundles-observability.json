{
  "title": "Bundle Adoption & Activation",
  "tags": ["bundles", "observability"],
  "schemaVersion": 38,
  "refresh": "5m",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "name": "Bundles SLO Incidents",
        "type": "grafana-oncall",
        "datasource": "grafana-oncall",
        "enable": true,
        "iconColor": "rgba(255, 96, 96, 1)",
        "query": "{\"route\":\"bundles-slo\"}"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "workspace",
        "type": "query",
        "datasource": "observability-prometheus",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "query": "label_values(bmad_web_bundle_view_total, workspace)",
        "current": {
          "text": "All",
          "value": ".*"
        }
      },
      {
        "name": "bundle",
        "type": "query",
        "datasource": "observability-prometheus",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "query": "label_values(bmad_core_bundle_activation_total, bundle_id)",
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "links": [
    {
      "title": "OnCall Route - Bundles SLO",
      "url": "https://grafana-oncall.example.com/routes/bundles-slo",
      "targetBlank": true,
      "type": "link"
    }
  ],
  "panels": [
    {
      "type": "stat",
      "title": "Bundle Views (15m rate)",
      "id": 1,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 2
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(bmad_web_bundle_view_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[15m]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Bundle Launches (15m rate)",
      "id": 2,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 2
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(bmad_web_bundle_launch_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[15m]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Views Semanais por Bundle",
      "id": 8,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 300
              },
              {
                "color": "green",
                "value": 350
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(bmad_web_bundle_view_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[7d]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Weekly Activations por Bundle",
      "id": 3,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (bundle_id) (increase(bmad_core_bundle_activation_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[$__range]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "P95 Lead Time para Activação",
      "id": 4,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 12
              }
            ]
          }
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(bmad_core_bundle_activation_lead_time_hours_bucket{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[5m])) by (le))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Conversão Launch / View",
      "id": 5,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.22
              },
              {
                "color": "green",
                "value": 0.25
              }
            ]
          }
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(bmad_web_bundle_launch_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[$__range])) / sum(increase(bmad_web_bundle_view_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[$__range]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Workspaces activos (7d)",
      "id": 6,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count(sum_over_time(bmad_web_bundle_view_total{bundle_id=~\"$bundle\"}[7d]) > bool 0)"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Coverage por Workspace (7d views)",
      "id": 7,
      "datasource": "observability-prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "displayMode": "gradient",
        "min": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum_over_time(bmad_web_bundle_view_total{workspace=~\"$workspace\", bundle_id=~\"$bundle\"}[7d]))"
        }
      ]
    }
  ]
}
