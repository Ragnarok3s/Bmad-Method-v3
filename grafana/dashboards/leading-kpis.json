{
  "id": null,
  "uid": "leading-kpis",
  "title": "Leading KPIs Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5m",
  "style": "dark",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "panels": [
    {
      "type": "timeseries",
      "title": "Core - Listing Publish Lead Time",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "avg_over_time(bmad_leading_kpi_core_listing_publish_lead_time_hours{region=~\"$region\"}[24h])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 6 },
              { "color": "red", "value": 12 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Payments - Authorization Success Rate",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "avg_over_time(bmad_leading_kpi_payments_authorization_success_rate{region=~\"$region\"}[1h])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 98 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Identity - Proofing Conversion Rate",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "avg_over_time(bmad_leading_kpi_identity_identity_proofing_conversion_rate{flow=~\"$flow\"}[6h])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 85 },
              { "color": "green", "value": 92 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Property - Pricing Refresh Coverage",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "bmad_leading_kpi_property_pricing_refresh_rate{market=~\"$market\"}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 95 }
            ]
          }
        },
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "region",
        "label": "Region",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(bmad_leading_kpi_core_listing_publish_lead_time_hours, region)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "hide": 0
      },
      {
        "name": "flow",
        "label": "Identity Flow",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(bmad_leading_kpi_identity_identity_proofing_conversion_rate, flow)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "hide": 0
      },
      {
        "name": "market",
        "label": "Market",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(bmad_leading_kpi_property_pricing_refresh_rate, market)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "hide": 0
      }
    ]
  },
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5m", "15m", "1h", "6h", "12h", "1d"]
  }
}
